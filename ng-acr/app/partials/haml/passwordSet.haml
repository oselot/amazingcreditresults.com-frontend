.col-lg-4.col-lg-offset-4.col-xs-12
  %h4.text-center Welcome to Amazing Credit Results
  %form(ng-submit="form.setPassword()")
    .alert.alert-danger(ng-bind="form.message" ng-show="form.message")
    .form-group
      %label(for="pw") Password
      %input#pw.form-control(type="password" ng-model="form.model.password" tabindex="2")
    .form-group
      %a(href="/#/login") Back to Login
      %input.pull-right.btn.btn-success(type="submit" value="setPassword" tabindex="3")
